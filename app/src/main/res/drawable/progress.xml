<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5dip" />
            <gradient
                android:angle="270"
                android:centerColor="@color/progressBackgroundCenter"
                android:centerY="0.75"
                android:endColor="@color/progressBackgroundEnd"
                android:startColor="@color/progressBackgroundStart" />
        </shape>
    </item>

    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5dip" />
                <gradient
                    android:angle="270"
                    android:centerColor="@color/secondaryProgressCenter"
                    android:centerY="0.75"
                    android:endColor="@color/secondaryProgressEnd"
                    android:startColor="@color/secondaryProgressStart" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5dip" />
                <gradient
                    android:angle="270"
                    android:endColor="@color/progressEndColor"
                    android:startColor="@color/progressStartColor" />
            </shape>
        </clip>
    </item>

</layer-list>